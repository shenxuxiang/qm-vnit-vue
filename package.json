    "@types/echarts": "^4.9.18",
    "ant-design-vue": "^4.0.0",
    "axios": "^1.4.0",
    "echarts": "^5.4.3",
    "jssha": "^3.3.1",
    "mockjs": "^1.1.0",
    "path-to-regexp": "^6.2.1",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-runtime": "^7.22.15",
