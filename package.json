{
  "name": "vue-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepear": "husky install",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.dev.cjs",
    "start": "vite",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.prod.cjs",
    "preview": "vite preview",
    "lint": "npx eslint ./src --config ./.eslintrc.yml --ext .vue,.ts,.tsx --fix",
    "prettier": "npx prettier -w ./src",
    "lib-es": "npx gulp"
  },
  "lint-staged": {
    "src/**/*{.vue, .tsx, .ts, .jsx, .js}": [
      "npm run lint",
      "npm run prettier",
      "git add ."
    ]
