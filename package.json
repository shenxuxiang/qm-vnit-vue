{
  "name": "vue-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepear": "husky install",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.dev.cjs",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "npx eslint ./src --config ./.eslintrc.yml --ext .vue,.ts,.tsx --fix",
    "prettier": "npx prettier -w ./src"
  },
  "lint-staged": {
    "src/**/*{.vue, .tsx, .ts, .jsx, .js}": [
      "npm run lint",
      "npm run prettier",
      "git add ."
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.22.17",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
