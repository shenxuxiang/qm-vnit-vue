    "html-webpack-plugin": "^5.5.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "postcss": "^8.4.30",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.4",
    "progress-bar-webpack-plugin": "^2.1.0",
    "style-loader": "^3.3.3",
    "typescript": "^5.2.2",
    "vue-loader": "^17.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
